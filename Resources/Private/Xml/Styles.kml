<Style id="defLineStyle">
    <LineStyle>
        <color>7f00ffff</color>
        <width>2</width>
    </LineStyle>
    <PolyStyle>
        <color>7f00ff00</color>
    </PolyStyle>
</Style>
<StyleMap id="defPointStyle">
